-var sentNum=3
form(method='post', action='addSentencesToLU')
    fieldset
        legend general data
        br
        key frameid
            input(type='text', name='data[frameid]', value=777777)
        br
        key luid
            input(type='text', name='data[luid]', value=150)
        key db
            input(type='text', name='data[db]', value='haaretz')
        input(type='text', name='data[source]', value='manual', hidden)
    fieldset
        legend sentences
        //key sentence1  <br>
            key content
                input(type='text', name='sentences[0][content]',value="#{exmSent}")
            key bad segmentation?
                input(type='checkbox', name='sentences[0][badseg]')
            key addTOLU
                input(type='checkbox', name='sentences[0][addtolu]')
            key inDB
                input(type='checkbox', name='sentences[0][indb]')
        //key sentence2  <br>
            key content
                input(type='text', name='sentences[1][content]', value="#{exmSent}")
            key bad segmentation
                input(type='checkbox', name='sentences[1][badseg]')
            key addTOLU
                input(type='checkbox', name='sentences[1][addtolu]')
            key inDB
                input(type='checkbox', name='sentences[1][indb]')
        //key sentence3  <br>
            key content
                input(type='text', name='sentences[2][content]', value="#{exmSent}")
            key bad segmentation
                input(type='checkbox', name='sentences[2][badseg]')
            key addTOLU
                input(type='checkbox', name='sentences[2][addtolu]')
            key inDB
                input(type='checkbox', name='sentences[2][indb]')
        -for(var i=0; i<sentNum; i++)
            b <br> sentence#{i}  <br>
            key content
                input(type='text', name='sentences[#{i}][content]', value="#{exmSent}")
            key <br> action
                select(name='sentences[#{i}][action]')
                    option(value='nothing') nothing
                    option(value='addtolu') addtolu
                    option(value='badseg') mark badseg
            br

    input(type='submit')